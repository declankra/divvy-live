gcloud functions deploy divvy-api \
  --gen2 \
  --runtime=python312 \
  --region=us-central1 \
  --source=gcp-functions/api \
  --entry-point=latest \
  --timeout=30s \
  --memory=128Mi \
  --set-env-vars=BUCKET_NAME=divvy-live-us-central1,GCP_PROJECT=divvy-460820 \
  --trigger-http \
  --allow-unauthenticated